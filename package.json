{
  "name": "scholargenerator",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "nodemon development": "npx nodemon %NODE_DEBUG_OPTION%"
  },
  "author": "Abourass <abourassa@assetval.com>",
  "license": "Private",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.9.0",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.1",
    "express-session": "^1.15.6",
    "fs": "0.0.1-security",
    "helmet": "^3.15.1",
    "http-errors": "^1.7.2",
    "ip": "^1.1.5",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-security": "^1.4.0",
    "google-closure-compiler": "^20190215.0.1",
    "mocha": "^6.0.1",
    "nodemon": "^1.18.10",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0"
  },
  "eslintConfig": {
    "plugins": [
      "security"
    ],
    "extends": [
      "airbnb-base",
      "plugin:node/recommended",
      "plugin:security/recommended"
    ],
    "env": {
      "shared-node-browser": true,
      "node": true,
      "es6": true,
      "mongo": true,
      "jquery": true,
      "jest": true
    },
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "yoda": 0,
      "no-unused-vars": 0,
      "one-var": 0,
      "one-var-declaration-per-line": 0,
      "consistent-return": 0,
      "linebreak-style": 0,
      "eqeqeq": 0,
      "func-names": 0,
      "import/newline-after-import": 0,
      "max-len": 0,
      "no-console": 0,
      "no-lonely-if": 0,
      "no-new-object": [
        "error"
      ],
      "object-curly-newline": [
        "error",
        {
          "ObjectPattern": {
            "multiline": true
          }
        }
      ],
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "no-restricted-properties": 0,
      "no-shadow": 0,
      "no-underscore-dangle": 0,
      "no-void": 0,
      "object-curly-spacing": 0,
      "object-shorthand": [
        "error",
        "consistent"
      ],
      "prefer-destructuring": 0,
      "space-before-blocks": 0,
      "space-before-function-paren": 0
    }
  }
}
